{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/auth/(.*)", 
        "dest": "/api/auth/$1" 
      },
      {
        "src": "/api/todo/(.*)", 
        "dest": "/api/todo/$1" 
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  
  